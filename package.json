    "demo": "https://hoia.hauntedthemes.com/",
    "version": "3.0.0",
    "license": "GNU General Public License, Version 2",
    "engines": {
        "ghost": ">=3.0.0",
        "ghost-api": "v3"
    },
    "author": {
        "name": "Haunted Themes",
        "email": "contact@hauntedthemes.com",
        "url": "https://www.hauntedthemes.com"
    },
    "gpm": {
        "type": "theme",
        "categories": [
            "Travel"
        ]
